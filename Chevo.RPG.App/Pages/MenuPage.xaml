<Page x:Class="Chevo.RPG.App.Pages.MenuPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chevo.RPG.App"
        xmlns:common="clr-namespace:Chevo.RPG.Core.Enum;assembly=Chevo.RPG.Core"
        mc:Ignorable="d"
        Title="MainWindow" Background="Black" ShowsNavigationUI="False">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <!--Add columns later-->
        </Grid.ColumnDefinitions>
        <Button Grid.Row="1" Content="Show available levels" Command="{Binding ViewLevels}"></Button>
    </Grid>
</Page>
